{
  "name": "typecon",
  "version": "0.6.1",
  "description": "Compiles Typescript code into CON",
  "repository": "https://github.com/MarcosPCB/TypeCon",
  "main": "./dist/main.js",
  "author": "ItsMarcos",
  "license": "GPL-3.0-only",
  "bin": {
    "tcc": "dist/main.js"
  },
  "scripts": {
    "dev": "IS_DEV=true ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/main.ts",
    "dev-mac": "IS_DEV=true ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/main.ts -c \"test/data/test.ts\" -of \"test/eduke32/Eduke32.app/Contents/Resources\" -ld",
    "dev-win": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/main.ts",
    "dev-win-debug": "ts-node-dev --inspect-brk --transpile-only --ignore-watch node_modules src/main.ts",
    "dev-debug": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/main.ts -debug eduke32.exe",
    "parse": "IS_DEV=true ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/main.ts -c \"test/data/test.ts\" -p",
    "debug": "IS_DEV=true ts-node-dev --inspect-brk --transpile-only --ignore-watch node_modules src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "postbuild": "node postBuild.js",
    "release-patch": "yarn run build && yarn version --patch && git push && git push --tags",
    "release-minor": "yarn run build && yarn version --minor && git push && git push --tags",
    "release-major": "yarn run build && yarn version --major && git push && git push --tags"
  },
  "keywords": [
    "log",
    "useful",
    "logger",
    "level",
    "debug",
    "debugging",
    "console",
    "error",
    "warning"
  ],
  "homepage": "https://github.com/MarcosPCB/TypeCon",
  "bugs": "https://github.com/MarcosPCB/TypeCon/issues",
  "files": [
    "dist/main.js",
    "dist/*.js",
    "dist/*.ts",
    "dist/sets/*.js",
    "dist/sets/**/*.js",
    "dist/modules/*.js",
    "dist/modules/**/*.js",
    "include/*.*",
    "include/**/*.*",
    "templates/*.*",
    "templates/**/*.*",
    "README.md",
    "postBuild.js"
  ],
  "dependencies": {
    "fs-extra": "^11.3.0",
    "inquirer": "^12.5.0",
    "semver": "^7.7.2",
    "ts-morph": "^25.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
